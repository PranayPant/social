<div id="welcome">

	<div class="title">
		<h1>Hub</h1>
	</div>

	<div class="main-links">
		<%=link_to "Sign up", new_user_registration_path, :class => "btn btn-default" %>
		<%=link_to "Log in", new_user_session_path, :class => "btn btn-primary"  %>
	</div>

	<div class="log-out">
		<% if user_signed_in? %>
			<%= link_to "Log out", destroy_user_session_path, :method => :delete, :class => "btn btn-danger" %>
		<% end %>
	</div>

</div>
	
